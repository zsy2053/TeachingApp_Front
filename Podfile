target 'iTA' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'SwiftyJSON', '~> 4.0'
  pod 'Alamofire', '~> 4.7'
  pod 'DLRadioButton', '~> 1.4'
  # Pods for iTA

  target 'iTATests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iTAUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
